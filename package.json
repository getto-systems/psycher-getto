{
  "version": "0.0.0",
  "scripts": {
    "start": "npm run watch",
    "watch": "npm run watch:lib & npm run watch:test",
    "watch:lib": "exec-on-changed -d ./lib -f ./bin/test_corresponding.sh",
    "watch:test": "exec-on-changed -d ./test -f ./bin/test.sh",
    "test": "jest --config jest.config.json"
  },
  "author": "shun@getto.systems",
  "license": "MIT",
  "devDependencies": {
    "exec-on-changed": "^1.2.0",
    "jest": "^24.8.0"
  },
  "dependencies": {
    "aws-sdk": "^2.502.0",
    "node-fetch": "^2.6.0"
  }
}
